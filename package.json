{
  "name": "webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "NODE_ENV=production npx babel --extensions '.tsx,.ts,.jsx,.js' src --ignore src/sequelize/config,src/sequelize/migrations,src/sequelize/seeders,src/seed,src/public,src/tests,src/types -d public",
    "test": "jest --clearCache && jest src",
    "Start Production (F9)": "clear && NODE_ENV=production node dist/index.js\n",
    "Start Development (F10)": "clear && NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/index.ts\n",
    "Build & Run - Prod (Ctrl + F9)": "clear && NODE_ENV=production npx babel --extensions '.tsx,.ts,.jsx,.js' src --ignore src/sequelize/config,src/sequelize/migrations,src/sequelize/seeders,src/seed,src/public,src/tests,src/types -d dist && NODE_ENV=production node dist/index.js\n",
    "Build & Run - Dev (Ctrl + F10)": "clear && NODE_ENV=development npx webpack --env buildType=development --mode development & NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/index.ts\n",
    "Seed": "clear && npx sequelize-cli db:seed:all",
    "Undo-Seed": "clear && npx sequelize-cli db:seed:undo:all",
    "Refresh-Seed": "clear && npm run Undo-Seed && npm run Seed",
    "Migrate": "clear && npx sequelize-cli db:migrate",
    "Undo-Migrate": "clear && npx sequelize-cli db:migrate:undo:all",
    "Clean-Slate": "clear && npm run Undo-Migrate && npm run Migrate",
    "Clean Slate with Data": "clear && npm run Clean-Slate && npm run Seed",
    "Reset-Database": "clear && npx cross-env ACTION=reset ts-node scripts/Database.ts && npm run Migrate && npm run Seed",
    "Create-Database": "clear && npx cross-env ACTION=create ts-node scripts/Database.ts",
    "sequelize-push-schema": "clear && npx cross-env ACTION=create ts-node ./src/sequelize/scripts/SyncTables.ts",
    "sequelize-pull-schema": "clear && npx cross-env ACTION=create ts-node ./src/sequelize/scripts/SequelizeAuto.js",
    "Drop-Database": "clear && npx cross-env ACTION=drop ts-node scripts/Database.ts"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
